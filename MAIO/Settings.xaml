<UserControl x:Class="MAIO.Settings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MAIO"
             mc:Ignorable="d" Height="768" Width="1080">
    <UserControl.Resources>
        <ControlTemplate x:Key="FilletTextBox" TargetType="{x:Type TextBox}">
            <Border BorderBrush="#FF70B8EE" BorderThickness="1" CornerRadius="5">
                <ScrollViewer x:Name="PART_ContentHost" VerticalAlignment="Center"/>
            </Border>
        </ControlTemplate>
    </UserControl.Resources>
    <Grid Background="#FF222136" Width="1080">
        <Button Content="X" HorizontalAlignment="Left" Height="30" Margin="1036,0,0,0" VerticalAlignment="Top" Width="44" Background="#FF2A2C48" BorderBrush="{x:Null}"  BorderThickness="0"  Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}"  Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" FontFamily="Arial" Click="Button_Click" FontSize="20" RenderTransformOrigin="0.676,0.332"/>
        <DockPanel LastChildFill="False" Margin="81,91,81,91" Background="#FF2A2C48">
            <Label Content="Discord Webhook" FontSize="16" FontFamily="PingFangSC-Semibold" Foreground="White" DockPanel.Dock="Top" Height="34" Width="158" Margin="-450,29,0,15" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
            <TextBox Template="{StaticResource FilletTextBox}" x:Name="discordwebhook" BorderThickness="0" Height="34" TextWrapping="NoWrap" Background="#FF9CB0C6" MaxLines="1" FontFamily="PingFangSC-Semibold" FontSize="16" Foreground="White" DockPanel.Dock="Top" Width="400" Margin="0,-70,-318,0"/>
            <Label Content="Advanced Mode" FontSize="16" FontFamily="PingFangSC-Semibold" Foreground="White" RenderTransformOrigin="0.515,-2.047" DockPanel.Dock="Top" Height="34" Width="160" Margin="-445,11,0,15"/>
            <CheckBox Content="" x:Name="useAdvancemode" DockPanel.Dock="Top" Height="34" Width="41" Margin="-41,-52,0,0"/>
            <Label Content="Delay" FontSize="16" FontFamily="PingFangSC-Semibold" Foreground="White" DockPanel.Dock="Top" Height="34" Width="145" Margin="-463,7,0,15"/>
            <TextBox Template="{StaticResource FilletTextBox}" x:Name="delay2" Height="33" TextWrapping="NoWrap" FontFamily="PingFangSC-Semibold" FontSize="16" Background="#FF9CB0C6" Foreground="White" DockPanel.Dock="Top" Width="108" Margin="27,-70,0,0" HorizontalAlignment="Stretch"/>
            <Label Content="Use Monitor" FontSize="16" FontFamily="PingFangSC-Semibold" Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" RenderTransformOrigin="0.515,-2.047" DockPanel.Dock="Top" Height="34" Width="108" Margin="-500,15,0,15"/>
            <CheckBox Content="" x:Name="Usemonitor" DockPanel.Dock="Top" Height="34" Margin="-51,-51,0,0" Width="31"/>
            <Label Content="Cookie Num" FontFamily="PingFangSC-Semibold" FontSize="16" HorizontalContentAlignment="Center" Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" VerticalContentAlignment="Center" DockPanel.Dock="Top" Height="34" Width="100" Margin="-509,8,0,15"/>
            <Button Cursor="Hand" x:Name="button1_Copy1" Opacity="1" Foreground="White" Background="#FF6F7D8D" Content="Start Gen" FontFamily="PingFangSC-Semibold" FontSize="16" Click="gencookie" RenderTransformOrigin="0.13,0.444" DockPanel.Dock="Top" Height="45" Width="108" Margin="25,-64,0,0">
                <Button.Template>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border BorderBrush="{TemplateBinding BorderBrush}" x:Name="PART_Background" CornerRadius="5,5,5,5" Height="33">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                                    <GradientStop Color="#FF70B8EE" Offset="0.0" />
                                </LinearGradientBrush>
                            </Border.Background>
                            <ContentPresenter Content="{TemplateBinding Content}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                    </ControlTemplate>
                </Button.Template>
            </Button>
            <Button Cursor="Hand" x:Name="DelAllCookie" Opacity="1" Foreground="White" Background="#FF6F7D8D" Content="Del Cookie" FontFamily="PingFangSC-Semibold" FontSize="16" Click="DelAllCookie_Click" RenderTransformOrigin="0.13,0.444" DockPanel.Dock="Top" Height="45" Width="108" Margin="6,-64,-378,0">
                <Button.Template>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border BorderBrush="{TemplateBinding BorderBrush}" x:Name="PART_Background" CornerRadius="5,5,5,5" Height="33">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                                    <GradientStop Color="#FF70B8EE" Offset="0.0" />
                                </LinearGradientBrush>
                            </Border.Background>
                            <ContentPresenter Content="{TemplateBinding Content}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                    </ControlTemplate>
                </Button.Template>
            </Button>
            <TextBlock Text="AutoClearCookie" TextWrapping="Wrap" RenderTransformOrigin="3.636,-6.565" FontFamily="PingFangSC-Semibold" FontSize="16" Foreground="White" DockPanel.Dock="Top" Height="34" Width="125" Margin="-471,19,0,15"/>
            <CheckBox Content="" x:Name="autoclearcookie" DockPanel.Dock="Top" Height="16" Width="66" Margin="-19,-78,0,0"/>
            <Label Content="Cid" FontSize="16" FontFamily="PingFangSC-Semibold" Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" DockPanel.Dock="Top" Height="34" Width="88" Margin="-519,6,0,15"/>
            <TextBox Template="{StaticResource FilletTextBox}" x:Name="Cid" Height="33" TextWrapping="NoWrap" FontFamily="PingFangSC-Semibold" FontSize="16" Background="#FF9CB0C6" RenderTransformOrigin="0.512,1.596" Foreground="White" DockPanel.Dock="Top" Width="108" Margin="23,-69,0,0"/>
            <Label Content="Cjevent" FontSize="16" FontFamily="PingFangSC-Semibold" Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" DockPanel.Dock="Top" Height="34" Width="88" Margin="-1,-64,-355,0"/>
            <Label Content="Version: 0.99.12" FontFamily="PingFangSC-Semibold" FontSize="16" Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" RenderTransformOrigin="0.489,0.706" DockPanel.Dock="Top" Height="34" Width="137" Margin="-469,11,0,0"/>
            <Label Content="Expiry: Renew" FontFamily="PingFangSC-Semibold" FontSize="16" Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" DockPanel.Dock="Top" Height="34" Width="137" Margin="0,-34,-401,0"/>
            <TextBox Template="{StaticResource FilletTextBox}" x:Name="Cjevent" Height="33" TextWrapping="NoWrap" FontFamily="PingFangSC-Semibold" FontSize="16" Background="#FF9CB0C6" RenderTransformOrigin="0.466,1.778" Foreground="White" DockPanel.Dock="Top" Width="108" Margin="0,-159,-602,0"/>
            <Button Cursor="Hand" x:Name="save" Opacity="1" Foreground="White" Background="#FF6F7D8D" Content="Save" FontFamily="PingFangSC-Semibold" FontSize="16" Click="save_Click" DockPanel.Dock="Top" Height="48" Width="108" Margin="0,2,0,0">
                <Button.Template>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border BorderBrush="{TemplateBinding BorderBrush}" x:Name="PART_Background" CornerRadius="5,5,5,5" Height="33">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                                    <GradientStop Color="#FF70B8EE" Offset="0.0" />
                                </LinearGradientBrush>
                            </Border.Background>
                            <ContentPresenter Content="{TemplateBinding Content}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                    </ControlTemplate>
                </Button.Template>
            </Button>
        </DockPanel>
        <Button Cursor="Hand" x:Name="Deactive" Opacity="1" Foreground="White" Background="#FFFF5439" Content="Deactive" FontFamily="PingFangSC-Semibold" FontSize="16" Click="Deactive_Click" Margin="954,699,22,27">
            <Button.Template>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" x:Name="PART_Background" CornerRadius="4,4,4,4">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                                <GradientStop Color="#FFFF5439" Offset="0.0" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <ContentPresenter Content="{TemplateBinding Content}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Border>
                </ControlTemplate>
            </Button.Template>
        </Button>
        <Label Content="Settings" FontFamily="PingFangSC-Semibold" HorizontalAlignment="Left" Height="55" VerticalAlignment="Top" Width="140" Margin="30,25,0,0" Foreground="White" FontSize="32" FontWeight="Bold"/>
    </Grid>
</UserControl>
