<Page x:Class="MAIO.Proxy"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MAIO"
             mc:Ignorable="d" Height="769" Width="961" VerticalAlignment="Bottom">
    <Grid Background="#FF243B55" Height="769" RenderTransformOrigin="0.534,0.484">
        <RichTextBox Margin="65,118,422,138" x:Name="testbox" BorderThickness="0" Background="#FF243B55" FontFamily="Arial" FontSize="14" Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}">
            <FlowDocument/>
        </RichTextBox>
        <StackPanel Background="#FF011F1F" Orientation="Horizontal" Margin="0,47,0,675" Height="47">
            <Label Content="Proxy" Margin="70,0,0,0" Height="32" Width="225" Background="#FF011F1F" Foreground="#FFF5EBEB" FontFamily="Arial" FontSize="16" VerticalAlignment="Center"/>
            <Label Content="Speed" Margin="100,0,0,0" Height="32" Width="98" Background="#FF011F1F" Foreground="#FFF5EBEB" FontFamily="Arial" FontSize="16"/>
            <Label Content="Import Proxies" HorizontalContentAlignment="Center" Height="32" Width="309" Background="#FF011F1F" Foreground="#FFF5EBEB" FontFamily="Arial" FontSize="16" VerticalAlignment="Center" Margin="132,0,0,0"/>
        </StackPanel>
        <Button Content="X" HorizontalAlignment="Left" Height="17" Margin="935,0,0,752" VerticalAlignment="Bottom" Width="26" Background="#FF243B55" BorderBrush="{x:Null}"  BorderThickness="0"  Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}"  Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" FontFamily="Arial" Click="Button_Click"/>
        <Button Cursor="Hand" x:Name="save" Opacity="1" Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" Background="#FF6F7D8D" Content="Save" FontFamily="Arial" FontSize="16" Margin="634,651,219,73" Width="108" Click="save_Click">
            <Button.Template>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="1" CornerRadius="10,10,10,10" x:Name="PART_Background">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                                <GradientStop Color="Transparent" Offset="0.0" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <ContentPresenter Content="{TemplateBinding Content}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Border>
                </ControlTemplate>
            </Button.Template>
        </Button>
        <Button Cursor="Hand"  x:Name="del" Opacity="1" Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" Background="#FF6F7D8D" Content="Delete" FontFamily="Arial" FontSize="16" Margin="843,651,10,73" Width="108" Click="del_Click">
            <Button.Template>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="1" CornerRadius="10,10,10,10" x:Name="PART_Background">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                                <GradientStop Color="Transparent" Offset="0.0" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <ContentPresenter Content="{TemplateBinding Content}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Border>
                </ControlTemplate>
            </Button.Template>
        </Button>
        <Button Cursor="Hand" Click="clearfailed_Click" x:Name="clearfailed" Opacity="1" Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" Background="#FF6F7D8D" Content="Clear Failed" FontFamily="Arial" FontSize="16" Margin="372,688,480,36" Width="108">
            <Button.Template>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="1" CornerRadius="10,10,10,10" x:Name="PART_Background">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                                <GradientStop Color="Transparent" Offset="0.0" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <ContentPresenter Content="{TemplateBinding Content}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Border>
                </ControlTemplate>
            </Button.Template>
        </Button>
        <Button Cursor="Hand" x:Name="test" Opacity="1" Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" Background="#FF6F7D8D" Content="Test All" FontFamily="Arial" FontSize="16" Margin="133,688,720,36" Width="108" Click="test_Click">
            <Button.Template>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="1" CornerRadius="10,10,10,10" x:Name="PART_Background">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                                <GradientStop Color="Transparent" Offset="0.0" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <ContentPresenter Content="{TemplateBinding Content}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Border>
                </ControlTemplate>
            </Button.Template>
        </Button>
        <RichTextBox Margin="616,118,13,138" Name="proxybox" Background="#FF292F34" Foreground="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" FontFamily="Arial" FontSize="14">
            <FlowDocument/>
        </RichTextBox>
    </Grid>
</Page>
